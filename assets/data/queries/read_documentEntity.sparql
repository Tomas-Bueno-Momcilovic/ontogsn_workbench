PREFIX schema: <https://schema.org/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?entity ?s ?p ?o
WHERE {
  BIND(__TAG__ AS ?tag)

  ?entity schema:identifier ?id .
  FILTER(CONTAINS(STR(?id), STR(?tag)))

  {
    ?entity ?p ?o .
    BIND(?entity AS ?s)
  }
  UNION
  {
    ?s ?p ?entity .
    BIND(?entity AS ?o)
  }
}
LIMIT 20
